{
  "name": "proexporter",
  "version": "0.0.10",
  "description": "Azure DevOps extension for exporting Work items in varous output formats",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "package:release": "tfx extension create --rev-version --manifest-globs azure-devops-extension.json",
    "package:current": "tfx extension create --manifest-globs azure-devops-extension.json",
    "publish": "tfx extension publish --manifest-globs azure-devops-extension.json",
    "build": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --mode development --hot --progress --static ./ --server-type https --port 8888",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./",
    "lint:fix": "eslint --ext .js,.jsx,.ts,.tsx ./ --fix && npm run prettier:write",
    "prettier:check": "npx prettier --check src/",
    "prettier:write": "npx prettier --write src/"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "azure-devops-extension-api": "^2.220.1",
    "azure-devops-extension-sdk": "^3.1.2",
    "azure-devops-ui": "^2.167.73",
    "buffer": "^6.0.3",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "stream": "^0.0.2",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "base64-inline-loader": "^2.0.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-unused-imports": "^2.0.0",
    "file-loader": "^6.2.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
