{
  "name": "proexporter",
  "version": "0.0.7",
  "description": "Azure DevOps extension for exporting Work items in varous output formats",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "compile": "npm run clean && webpack --mode production",
    "compile:dev": "npm run clean && webpack --mode development",
    "package:release": "tfx extension create --rev-version --manifest-globs azure-devops-extension.json",
    "package:current": "tfx extension create --manifest-globs azure-devops-extension.json",
    "publish": "tfx extension publish --manifest-globs azure-devops-extension.json",
    "build": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "webpack-dev-server --mode development --hot --progress --static ./ --server-type https --port 8888"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "azure-devops-extension-api": "^2.220.1",
    "azure-devops-extension-sdk": "^3.1.2",
    "azure-devops-ui": "^2.167.73",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/react": "~16.8.2",
    "@types/react-dom": "~16.8.0",
    "copy-webpack-plugin": "^11.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.1.3",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
