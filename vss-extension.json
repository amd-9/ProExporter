{
    "manifestVersion": 1,
    "id": "proexporter",
    "publisher": "",
    "version": "0.0.1",
    "name": "ProExporter",
    "description": "Azure DevOps extension for exporting Work items in various output formats",
    "public": false,
    "categories": ["Azure Repos"],
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "contributions": [
        {
            "id": "export-test-plan-menu-item",
            "type": "ms.vss-web.action",
            "targets": [
                "ms.vss-test-web.test-run-grid-menu"
            ],
            "properties": {
                "text": "Sample build definition menu item",
                "uri": "dist/Menu/Menu.html",
                "icon": {
                    "light": "static/export.png",
                    "dark": "static/export.png"
                },
                "registeredObjectId": "sample-build-menu"
            }
        }
    ],
    "files": [
        {
            "path": "my-hub.html",
            "addressable": true
        },
        {
            "path": "node_modules/azure-devops-extension-sdk/lib",
            "addressable": true,
            "packagePath": "lib"
        }
    ]
}